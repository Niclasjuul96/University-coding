CHIP Memory {
	IN in[16], load, address[15];
	OUT out[16];

	PARTS:
	DMux(in=load, sel=address[14],a=d0,b=d1);
	
	RAM16K(in[0..15]=in, load=d0,address[0..13]=address[0..13], out[0..15]=outram);
	
	Screen(in[0..15] = in, load=d1, address[0..12] = address[0..12], 	out[0..15] = outscreen);

	Keyboard(out[0..15] = outkbd);
	
	Mux4Way16(a = outram, b = outram, c = outscreen, d = outkbd, sel[0] = address[13], sel[1] = address[14], out=out);

}